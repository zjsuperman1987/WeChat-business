page {
  height: 100%;
  /* display: flex;
  flex-direction: column; */
}

x-scroll-view {
  height: calc(100% - 80px)
}

.view {
  height: 100px;
  line-height: 100px;
  text-align: center;
}


.home-top {
  width: 100%;
  height: 406rpx;
  padding: 20rpx 40rpx 0;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: 100% 100%;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #fff;
}

.mui-top {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 70rpx;
  width: 100%;
}
.search-bar {
  width: 480rpx;
  height: 100%;
  line-height: 70rpx;
  margin-right: 40rpx;
  background: #fff;
  border-radius: 35rpx;
  color: #222;
  padding-left: 10px;
  position: relative;
  box-sizing: border-box;
}
.search-bar icon {
  position: absolute;
  right: 10px;
  top: 19rpx;
  width: 32rpx;
  height: 32rpx;
  background: url(http://a3.qpic.cn/psb?/V12oEYDD0Tk57U/lLfLvPuHV3cubTY74s*N4CrMzyinA1Lpnp.zd*x.ZQ0!/m/dLYAAAAAAAAAnull&bo=IAAgAAAAAAADByI!&rf=photolist&t=5) no-repeat;
  background-size: 100% 100%;
}


.scan {
  width: 90rpx;
  height: 100%;
  margin-right: 30rpx;
  text-align: center;
}
.scan icon {
  display: block;
  width: 42rpx;
  height: 42rpx;
  margin: 0 auto;
  background: url(http://a4.qpic.cn/psb?/V12oEYDD0Tk57U/7kc6xujflAcskoONK9hCdu1bw1t7aiNCvI0fkaED5TM!/m/dL8AAAAAAAAAnull&bo=KQApAAAAAAADByI!&rf=photolist&t=5) no-repeat;
  background-size: 100% 100%;
}
  
.sign {
  width: 84rpx;
  height: 100%;
  text-align: center;
}
.sign icon {
  display: block;
  width: 42rpx;
  height: 42rpx;
  margin: 0 auto;
  background: url(http://a1.qpic.cn/psb?/V12oEYDD0Tk57U/p6vIEzPSAhtdq3gvNgl07lbPLPelYEy8zZaHWSRUY00!/m/dMQAAAAAAAAAnull&bo=JwAqAAAAAAADBy8!&rf=photolist&t=5) no-repeat;
  background-size: 100% 100%;
  position: relative;
} 
.sign icon icon {
  position: absolute;
  top: -2rpx;
  right: -10rpx;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: red;
}

.sign text, .scan text {
  font-size: 24rpx;
}

/* 轮播 */
 
swiper {
  height: 320rpx;
  width: 100%;
  margin-top: 20rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
swiper-item {
  border-radius: 20rpx;
  overflow: hidden;
}
swiper image {
  width: 100%;
  height: 100%;
}